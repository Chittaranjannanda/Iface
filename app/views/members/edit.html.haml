!!!
.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", members_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_member_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_member_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Member
    .inner
      = form_for @member, :url => member_path(@member), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')

